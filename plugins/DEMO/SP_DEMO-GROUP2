SP_DEMO-GROUP2()
{
  GetVar -pattern "Fail" -name FAIL -default ""
  Task "echo first task"
  StartGroup
    Task "sleep 35"
    Task "sleep 8"
    Task "printf \${RANDOM:4}"
    Task "sleep 21"
    (( FAIL==1 )) && Task "false"
  EndGroup
  
  Task "sleep 15"
  
  StartGroup
    Task "sleep 32"
    Task "sleep 12"
    Task "hostname"
    Task "sleep 22"
  EndGroup

  StartGroup
    Task "echo CFW-STOP"
    breakTask "CON-WCC"
    Task "echo EDS-VIEW-INFRA"
    Task "echo RMB-APP"
  EndGroup
  StartGroup
    Task "echo CON-APP"
    Task "echo EDS-OES-CORE"
  EndGroup
  StartGroup
    Task "echo EDS-CSS-CORE"
    Task "echo EDS-DBS-ALTER"
    Task "echo EDS-OES-CORE"
  EndGroup
  StartGroup
    Task "echo CSS-WLST"
    Task "echo EDS-ODS-CORE"
  EndGroup
  StartGroup
    Task "echo CFW-START"
    Task "echo EDS-NFR-CORE"
    Task "echo OCH-SDM-CORE"
  EndGroup
  Task "echo CHECK-SDM"
  StartGroup
    Task "EDS-FAH-CORE"
    Task "ODI-DBS-PRIV"
  EndGroup
  breakTask "ODI-TOP"
  StartGroup
    breakTask "ODI-APP"
    Task ODI-DBS-CORE
  EndGroup

}
